<configuration>

    <appender name="MMAppender" class="com.ssafy.algoFarm.exception.MMAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss} %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="error">
        <appender-ref ref="MMAppender"/>
    </root>

</configuration>
